{
  "name": "airtunes2",
  "description": "an AirTunes v2 implementation: stream wirelessly to audio devices.",
  "keywords": [
    "airtunes",
    "airplay",
    "raop",
    "coreaudio",
    "sound",
    "audio",
    "cider"
  ],
  "version": "2.3.1",
  "author": "vapormusic <vapormusic.service@gmail.com>",
  "main": "./lib/index.js",
  "bin": "./examples/play_ffmpeg.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ciderapp/node_airtunes2.git"
  },
  "scripts": {
    "build-binaries-electron": "run-script-os",
    "build-binaries-electron-ci": "prebuild --platform win32 --platform linux --platform darwin --include-regex \"^.*\\.(node|dylib|lib|exp|dll|so(\\.[0-9])?)$\" -r electron -t 19.0.1 -t 16.0.7",
    "build-binaries-electron:win32": "prebuild --platform win32 --include-regex \"^.*\\.(node|dylib|lib|exp|dll|so(\\.[0-9])?)$\" -r electron -t 19.0.3 -t 18.3.3 -t 16.0.7",
    "build-binaries-electron:linux": "prebuild --platform linux --include-regex \"^.*\\.(node|dylib|lib|exp|dll|so(\\.[0-9])?)$\" -r electron -t 19.0.3 -t 18.3.3",
    "build-binaries-electron:darwin": "prebuild --platform darwin --include-regex \"^.*\\.(node|dylib|lib|exp|dll|so(\\.[0-9])?)$\" -r electron -t 19.0.3 -t 18.3.3",
    "build-binaries-electron2": "prebuild --platform darwin -a arm64 --include-regex \"^.*\\.(node|dylib|lib|exp|dll|so(\\.[0-9])?)$\" -r electron -t 19.0.3 -t 18.3.3",
    "install": "prebuild-install || (npm run rebuild || (true || VER>NUL))",
    "rebuild": "node-gyp configure build"
  },
  "licenses": [
    {
      "type": "BSD"
    }
  ],
  "pkg": {
    "scripts": [
      "lib/homekit/*.js",
      "lib/*.js"
    ],
    "assets": [
      "build/Release/obj/airtunes/*",
      "node_modules/curve25519-n2/build/Release/*",
      "node_modules/sodium/build/Release/*",
      "node_modules/ed25519/build/Release/*"
    ],
    "targets": [
      "node14-win",
      "node14-macos-x64",
      "node14-linux-x64"
    ]
  },
  "dependencies": {
    "@noble/ed25519": "^1.6.1",
    "async": "^3.2.1",
    "axlsign": "git+https://github.com/wavesplatform/curve25519-js.git",
    "big-integer": "^1.6.25",
    "chacha-js": "https://github.com/davidwu226/chacha20poly1305",
    "crypto-js": "^4.1.1",
    "curve25519-js": "^0.0.4",
    "elliptic": "^6.4.0",
    "fast-srp-hap": "^2.0.4",
    "js-crypto-aes": "^1.0.4",
    "js-sha1": "^0.4.1",
    "lodash": "^4.17.4",
    "parse-raw-http": "0.0.1",
    "prebuild-install": "^7.1.0",
    "python-struct": "^1.1.3",
    "simple-plist": "^1.3.1",
    "varint": "^6.0.0",
    "yargs": "^9.0.1"
  },
  "devDependencies": {
    "optimist": "^0.6.1",
    "prebuild": "^11.0.4",
    "request": "^2.83.0",
    "run-script-os": "^1.1.6"
  },
  "bugs": {
    "url": "https://github.com/ciderapp/node_airtunes2/issues"
  }
}
